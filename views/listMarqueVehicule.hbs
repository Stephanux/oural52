{{#if msg}}
    <div id="modal">
        <div class="modal-content">
            <svg class="close-modal" xmlns="http://www.w3.org/2000/svg" data-name="Layer 2" viewBox="0 0 32 40"x="0px" y="0px">
                <path d="M4,29a1,1,0,0,1-.707-1.707l24-24a1,1,0,1,1,1.414,1.414l-24,24A1,1,0,0,1,4,29Z" />
                <path d="M28,29a1,1,0,0,1-.707-.293l-24-24A1,1,0,0,1,4.707,3.293l24,24A1,1,0,0,1,28,29Z" />
            </svg>
            <h3>{{msg}}</h3>
        </div>
    </div>     
{{/if}}
<div class="wrap">
    <div class="super-head">
        <button class="btn_down">
            <div>
                <span>Aller en bas</span>
                <svg viewBox="0 0 64 80" x="0px" y="0px"><path d="M24.73,47.56a2,2,0,0,0,2.83-2.83L14.83,32,27.56,19.27a2,2,0,0,0-2.83-2.83L10.59,30.59a2,2,0,0,0,0,2.83Z"/><path d="M52.14,30H26a2,2,0,0,0,0,4H52.14a2,2,0,0,0,0-4Z"/></svg>
            </div>
        </button>
        <table id="stock">
            <thead>
                <tr>
                    <th>Marque du v√©hicule</th>
                    <th>Edition</th>
                </tr>
            </thead>
            <tbody>
                {{#each data.marques}}
                <tr>
                    <td>
                        <div class="printResp" data-label="Nom de la marque"></div>
                        <div>
                            {{this.nom_marque}}
                        </div>
                    </td>
                    <td>
                        <div class="printResp" data-label="Edition"></div>
                        <div>
                            <span>
                                <a href="/formEditMarqueVehicule?id={{this.id_marque}}"><svg class="edit" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 95 95" style="enable-background:new 0 0 95 95" xml:space="preserve"><path d="m11.089 63.472 46.88-46.88 20.506 20.505-46.88 46.881zM61.767 12.705 74.495-.022 95 20.484 82.273 33.21zM5.6 68.9 0 95l26.1-5.6 1.6-1.6L7.2 67.3z"/></svg></a>
                                <a href="/formDeleteMarqueVehicule?id={{this.id_marque}}"><svg class="delete" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 26 26" style="enable-background:new 0 0 26 26" xml:space="preserve"><path d="M1 26c-.6 0-1-.4-1-1 0-.3.1-.5.3-.7l24-24c.4-.4 1-.4 1.4 0s.4 1 0 1.4l-24 24c-.2.2-.4.3-.7.3z"/><path d="M25 26c-.3 0-.5-.1-.7-.3l-24-24C-.1 1.3-.1.7.3.3c.4-.4 1-.4 1.4 0l24 24c.4.4.4 1 0 1.4-.2.2-.4.3-.7.3z"/></svg></a>
                            </span>
                        </div>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        <div>
            <button class="back">
                <div>
                    <svg class="btnBack" data-name="Layer 3" viewBox="0 0 64 80" x="0px" y="0px"><path d="M24.73,47.56a2,2,0,0,0,2.83-2.83L14.83,32,27.56,19.27a2,2,0,0,0-2.83-2.83L10.59,30.59a2,2,0,0,0,0,2.83Z"/><path d="M52.14,30H26a2,2,0,0,0,0,4H52.14a2,2,0,0,0,0-4Z"/></svg>
                    <span>Retour</span>
                </div>
            </button>
            <button class="btn_add">
                <div>
                    <svg class="add" x="0px" y="0px" viewBox="0 0 100 125"><g><path d="M20.7,20.7c-16.2,16.2-16.2,42.5,0,58.7c16.2,16.2,42.5,16.2,58.7,0c16.2-16.2,16.2-42.5,0-58.7   C63.2,4.5,36.8,4.5,20.7,20.7z M74.5,74.5C61,88,39,88,25.5,74.5s-13.5-35.5,0-49s35.5-13.5,49,0S88,61,74.5,74.5z"/><path d="M50,30.3L50,30.3c-1.9,0-3.4,1.5-3.4,3.4v13l-13,0c-1.9,0-3.4,1.5-3.4,3.4c0,1.9,1.5,3.4,3.4,3.4h13v13   c0,1.9,1.5,3.4,3.4,3.4c0.9,0,1.8-0.4,2.4-1c0.6-0.6,1-1.5,1-2.4v-13h13c0.9,0,1.8-0.4,2.4-1s1-1.5,1-2.4c0-1.9-1.5-3.4-3.4-3.4   h-13v-13C53.4,31.8,51.9,30.3,50,30.3z"/></g></svg>
                    <span>Ajouter</span>
                </div>
            </button>
        </div>
        <div id="ancre"></div>
    </div>
</div>
<script src="js/stock.js"></script>
<script src="js/modal/modalWrap.js" type="module"></script>