<div class="form-wrap">
    <div>
        <form action="{{this.form_action}}" method="POST" enctype="multipart/form-data">
            <div class="form">
                <label for="type_vehicule">Type de véhicule :</label>
                <input type="text" name="type_vehicule" id="type_vehicule" value="{{data.nom_type_vehicule}}">
                <label for="doc_pdf">Doc du type de véhicule :</label>
                <input type="file" name="doc_pdf" id="doc_pdf" value="{{data.doc_pdf}}">
                <input type="hidden" name="doc_pdf" value="{{data.doc_pdf}}">
                <input type="hidden" name="id" value="{{data.id_type_vehicule}}">
                <input type="submit" value="Modifier">
            </div>
        </form>
    </div>
    <div>
        <button class="back">
            <div>
                <svg class="btnBack" data-name="Layer 3" viewBox="0 0 64 80" x="0px" y="0px"><path d="M24.73,47.56a2,2,0,0,0,2.83-2.83L14.83,32,27.56,19.27a2,2,0,0,0-2.83-2.83L10.59,30.59a2,2,0,0,0,0,2.83Z"/><path d="M52.14,30H26a2,2,0,0,0,0,4H52.14a2,2,0,0,0,0-4Z"/></svg>
                <span>Retour</span>
            </div>
        </button>
    </div>
</div>