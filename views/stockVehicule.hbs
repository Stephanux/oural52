<div class="wrap">
    <div class="super-head">
        {{#if msg}}
        <h3>{{msg}}</h3>
        {{/if}}
        <div>
            <a href="/formInsertVehicule"><svg class="add" version="1.1" x="0px" y="0px" viewBox="0 0 100 125" style="enable-background:new 0 0 100 100;" xml:space="preserve"><g><path d="M20.7,20.7c-16.2,16.2-16.2,42.5,0,58.7c16.2,16.2,42.5,16.2,58.7,0c16.2-16.2,16.2-42.5,0-58.7   C63.2,4.5,36.8,4.5,20.7,20.7z M74.5,74.5C61,88,39,88,25.5,74.5s-13.5-35.5,0-49s35.5-13.5,49,0S88,61,74.5,74.5z"/><path d="M50,30.3L50,30.3c-1.9,0-3.4,1.5-3.4,3.4v13l-13,0c-1.9,0-3.4,1.5-3.4,3.4c0,1.9,1.5,3.4,3.4,3.4h13v13   c0,1.9,1.5,3.4,3.4,3.4c0.9,0,1.8-0.4,2.4-1c0.6-0.6,1-1.5,1-2.4v-13h13c0.9,0,1.8-0.4,2.4-1s1-1.5,1-2.4c0-1.9-1.5-3.4-3.4-3.4   h-13v-13C53.4,31.8,51.9,30.3,50,30.3z"/></g></svg></a>
        </div>
        <table id="stock">
            <thead>
                <tr>
                    <th>Nom du véhicule</th>
                    <th>Date mise en circulation</th>
                    <th>Marque du véhicule</th>
                    <th>Modèle</th>
                    <th>Type de véhicule</th>
                    <th>Doc véhicule</th>
                    <th>Edition</th>
                </tr>
            </thead>
            <tbody>
                {{#each data.Vehicules}}
                <tr>
                    <td>
                        <div class="printResp" data-label="Nom du véhicule"></div>
                        <div>
                            {{this.nom_vehicule}}
                        </div>
                    </td>
                    <td>
                        <div class="printResp" data-label="Mise en circulation"></div>
                        <div class="date">
                            {{this.date_mise_circulation}}
                        </div>
                    </td>
                    <td>
                        <div class="printResp" data-label="Marque du véhicule"></div>
                        <div>
                            {{this.nom_marque}}
                        </div>
                    </td>
                    <td>
                        <div class="printResp" data-label="Modèle"></div>
                        <div>
                            {{this.nom_modele}}
                        </div>
                    </td>
                    <td>
                        <div class="printResp" data-label="Type de véhicule"></div>
                        <div class="vehicule">
                            {{this.nom_type_vehicule}}
                        </div>
                    </td>
                    <td>
                        <div class="printResp" data-label="Doc véhicule"></div>
                        <div>
                            <a href="data/uploads/{{this.doc_pdf}}" target="_blank">{{this.doc_pdf}}</a>
                        </div>
                    </td>
                    <td>
                        <div class="printResp" data-label="Edition"></div>
                        <div>
                            <span>
                                <a href="/formEditVehicule?id={{this.id_vehicule}}"><svg class="edit" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 95 95" style="enable-background:new 0 0 95 95" xml:space="preserve"><path d="m11.089 63.472 46.88-46.88 20.506 20.505-46.88 46.881zM61.767 12.705 74.495-.022 95 20.484 82.273 33.21zM5.6 68.9 0 95l26.1-5.6 1.6-1.6L7.2 67.3z"/></svg></a>
                                <a href="/formDeleteVehicule?id={{this.id_vehicule}}"><svg class="delete" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 26 26" style="enable-background:new 0 0 26 26" xml:space="preserve"><path d="M1 26c-.6 0-1-.4-1-1 0-.3.1-.5.3-.7l24-24c.4-.4 1-.4 1.4 0s.4 1 0 1.4l-24 24c-.2.2-.4.3-.7.3z"/><path d="M25 26c-.3 0-.5-.1-.7-.3l-24-24C-.1 1.3-.1.7.3.3c.4-.4 1-.4 1.4 0l24 24c.4.4.4 1 0 1.4-.2.2-.4.3-.7.3z"/></svg></a>
                            </span>
                        </div>
                    </td>          
                </tr>
                {{/each}}
            </tbody>
        </table>
        <div>
            <button class="back">
                <div>
                    <svg class="btnBack" data-name="Layer 3" viewBox="0 0 64 80" x="0px" y="0px"><path d="M24.73,47.56a2,2,0,0,0,2.83-2.83L14.83,32,27.56,19.27a2,2,0,0,0-2.83-2.83L10.59,30.59a2,2,0,0,0,0,2.83Z"/><path d="M52.14,30H26a2,2,0,0,0,0,4H52.14a2,2,0,0,0,0-4Z"/></svg>
                    <span>Retour</span>
                </div>
            </button>
        </div>
    </div>
</div>
<script src="js/stock.js"></script>