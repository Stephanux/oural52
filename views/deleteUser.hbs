<div class="container">
    <div>
        {{#if msg}}
        <h4>{{msg}}</h4>
        {{/if}}
        <form action="{{form_action}}" method="POST" id="formDeleteUser">
            <div class="form">
                <label for="delete">Voulez-vous vraiment supprimer {{data.name}} - {{data.firstName}} ?</label>
                <input type="hidden" name="id" id="delete" value="{{data._id}}">
                <input type="submit" name="delete" value="Supprimer">
            </div>
        </form>
    </div>
    <div>
        <button class="back">
            <div>
                <svg class="btnBack" data-name="Layer 3" viewBox="0 0 64 80" x="0px" y="0px"><path d="M24.73,47.56a2,2,0,0,0,2.83-2.83L14.83,32,27.56,19.27a2,2,0,0,0-2.83-2.83L10.59,30.59a2,2,0,0,0,0,2.83Z"/><path d="M52.14,30H26a2,2,0,0,0,0,4H52.14a2,2,0,0,0,0-4Z"/></svg>
                <span>Retour</span>
            </div>
        </button>
    </div>
</div>